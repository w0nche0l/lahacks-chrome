{
  "manifest_version": 2,

  "name": "Wormhole Chrome Extension",
  "description": "This is a chrome extension for Wormhole",
  "version": "1.1",

  "permissions": [
    "background", 
    "cookies",
    "tabs",
    "http://*/*", 
    "https://*/*",
    "file://*/*"
  ],
    
  "background":{
    "page":"background.html"
  },

  "icons": { "16": "icon_16.png",
             "48": "icon_48.png",
             "128": "icon_128.png" },

  "key": "",

  "content_security_policy": "script-src 'self' https://www.rdio.com https://kloudless-static-assets.s3-us-west-2.amazonaws.com; object-src 'self'",
  "externally_connectable": {
      "matches": ["http://wormhole-api.cloudapp.net/*"]
  }
}
